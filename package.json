{
  "name": "reactive",
  "displayName": "Reactive",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/server/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "reactive.renderReact",
        "title": "Render React Tree"
      },
      {
        "command": "reactive.makeComponentTree",
        "title": "Make Component Tree"
      },
      {
        "command": "reactive.reactWebviewTree",
        "title": "React Component Tree in webview"
      }
    ]
  },
"scripts": {
    "build": "npm run compile && npm run webpack",     
    "compile": "tsc -p ./tsconfig.json",                
    "webpack": "webpack",                               
    "frontend:watch": "webpack --watch",               
    "backend:watch": "tsc -watch -p ./tsconfig.json",  
    "watch": "tsc -watch -p ./tsconfig.json",          
    "lint": "eslint src",                              
    "pretest": "npm run compile",                      
    "test": "node ./out/test/runTest.js"               
},

  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.25.9",
